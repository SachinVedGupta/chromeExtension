{
  "manifest_version": 3,
  "version": "1.0",
  "name": "CeeP",
  "action": {
      "default_popup": "index.html",
      "default_icon": "non-hidden-eye.png"
  },
  "permissions": [
      "tabs"
  ],
  "background": {
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ 
        "popup.js"
       ]
    }
  ]
}