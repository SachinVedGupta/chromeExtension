{
  "manifest_version": 3,
  "version": "1.0",
  "name": "CeeP",
  "action": {
      "default_popup": "index.html",
      "default_icon": "non-hidden-eye.png"
  },
  "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "commands": {
    "ruhhn-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Run \"foo\" on the current page."
    },
    "r-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+Z"
      },
      "description": "Run \"foo\" on the current page."
    }
  }
}